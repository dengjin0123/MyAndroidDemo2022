<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="WrapWrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="WrapWrap.normal_dark_black_text">
        <item name="android:textColor">@color/color_101010</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="WrapWrap.normal_black_text">
        <item name="android:textColor">@color/color_232323</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="normal_black_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_232323</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="normal_dark_black_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_101010</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="normal_white_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="large_dark_black_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_101010</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="large_black_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_232323</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="large_white_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">18sp</item>
    </style>

    <style name="small_dark_black_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_101010</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="small_black_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_232323</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="small_white_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="grey_line">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_1</item>
        <item name="android:background">@color/COLOR_F7F7F7</item>
    </style>

    <style name="common_btn">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/dp_40</item>
        <item name="android:background">@drawable/selector_common_btn</item>
        <item name="android:layout_marginStart">@dimen/dp_51</item>
        <item name="android:layout_marginEnd">@dimen/dp_51</item>
        <item name="android:textSize">@dimen/sp_16</item>
        <item name="android:textColor">@color/white</item>
    </style>

</resources>